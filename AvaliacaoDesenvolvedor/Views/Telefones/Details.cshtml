@model AvaliacaoDesenvolvedor.Models.Telefone

@{
    ViewData["Title"] = "Details";
}

<h1>Detalhes</h1>

<div>
    <h4>Telefone</h4>
    <hr />
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Numero</th>
                <th scope="col">Contato</th>
            </tr>
        </thead>
        <tbody>
            <td>@Html.DisplayFor(model => model.Numero)</td>
            <td>@Html.DisplayFor(model => model.Contato.Nome)</td>
        </tbody>
    </table>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">Editar</a> |
    <a asp-action="Index">Voltar para a lista</a>
</div>
